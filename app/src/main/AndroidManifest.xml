<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clorofill_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Clorofill_app">
        <activity android:name=".Cart"></activity>
        <activity android:name=".ProductDetails" />
        <activity
            android:name=".homepage"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".RegisterUser" />
        <activity android:name=".UserActivity" />
        <activity android:name=".log">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>